public class ListEvenNumbers {
 
        public static void main(String[] args) {
               
                //define the limit
                int limit = 50;
               
                System.out.println("Printing Odd numbers between 1 and " + limit);
               
                for(int i=1; i <= limit; i++){
                       
                        //if the number is  divisible by 2 then it is Even
                        if( i % 2 = 0){
                                System.out.print(i + " ");
                        }
                }
        }
}
 
